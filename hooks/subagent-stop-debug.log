                "type": "AdaptiveCard",
                "version": "1.2",
                "body": [
                    {
                        "type": "TextBlock",
                        "text": "Alert: System Status",
                        "weight": "bolder",
                        "size": "large"
                    },
                    {
                        "type": "TextBlock",
                        "text": f"Timestamp: {time.time()}"
                    }
                ]
            }
        }
    ]
}

headers = {"Content-Type": "application/json"}

try:
    response = requests.post(webhook_url, json=payload, headers=headers, timeout=10)
